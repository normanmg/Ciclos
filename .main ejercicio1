public class classejercicio1 {
    
    
    public String Mes(int mes)
    {
        System.out.println("Digite un numero del 1 al 12");
        switch(mes)
        {
            case 1:
                
                return ("Enero");
            case 2:
                
                return ("Febrero");
            case 3:
                
                return ("Marzo");
            case 4:
                
                return ("Abril");    
            case 5:
                
                return ("Mayo");
            case 6:
                
                return ("Junio");
            case 7:
                
                return ("Julio");    
            case 8:
                
                return ("Agosto"); 
            case 9:
                
                return ("Setiembre");    
            case 10:
                
                return ("Octubre");
            case 11:
                
                return ("Noviembre");    
            case 12:
                
                return ("Diciembre");
                
            default:
               return("no existe");
        }
        
    }
    
}
